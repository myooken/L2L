{
  "name": "L2L",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "test": "vitest run",
    "test:ci": "vitest run --silent",
    "preview": "vite preview",
    "licenses:scan": "license-checker --json > licenses.json && license-checker --production --relativeLicensePath --markdown > THIRD-PARTY-LICENSES.md",
    "licenses:check": "node _script/check-licenses.mjs",
    "licenses:notices": "node _script/collect-notices.mjs",
    "licenses:page": "node _script/build-license-page.mjs"
  },
  "dependencies": {
    "lz-string": "^1.5.0",
    "peerjs": "^1.5.5",
    "qrcode.react": "^4.2.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.10.0",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/lz-string": "^1.3.34",
    "@types/node": "^24.10.1",
    "@types/qrcode.react": "^1.0.5",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.1.1",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "jsdom": "^27.2.0",
    "license-checker": "^25.0.1",
    "spdx-expression-parse": "^3.0.1",
    "spdx-license-ids": "^3.0.20",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "vitest": "^4.0.15"
  },
  "overrides": {
    "parse5": "6.0.1"
  }
}
